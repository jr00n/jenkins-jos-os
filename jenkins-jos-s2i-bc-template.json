{
  "kind": "Template",
  "apiVersion": "v1",
  "metadata": {
    "name": "jenkins-jos-s2i"
  },
  "objects": [
    {
      "kind": "ImageStream",
      "apiVersion": "v1",
      "metadata": {
        "name": "jenkins-jos"
      },
      "spec": {},
      "status": {}
    },
    {
      "kind": "BuildConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "jenkins-jos-s2i"
      },
      "spec": {
        "triggers": [
          {
            "type": "ConfigChange"
          },
          {
            "type": "ImageChange",
            "imageChange": {}
          }
        ],
        "runPolicy": "Serial",
        "source": {
          "type": "Git",
          "git": {
            "uri": "${SOURCE_REPOSITORY_URL}"
          },
          "contextDir": "${CONTEXT_DIR}"
        },
        "strategy": {
          "type": "Source",
          "sourceStrategy": {
            "from": {
              "kind": "ImageStreamTag",
              "namespace": "openshift",
              "name": "jenkins:2"
            }
          }
        },
        "output": {
          "to": {
            "kind": "ImageStreamTag",
            "name": "jenkins-jos:latest"
          }
        },
        "resources": {},
        "postCommit": {}
      },
      "status": {}
    }
}
